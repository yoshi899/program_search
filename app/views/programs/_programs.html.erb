<% if @programs %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>番組ID</th>
        <th>開始時間</th>
        <th>終了時間</th>
        <th>チャンネル</th>
        <th>タイトル</th>
      </tr>
    </thead>
    <tbody>
        <% @programs.each do |program| %>
          <tr>
            <td><%= program[:program_id] %></td>
            <td><%= program[:start_time] %></td>
            <td><%= program[:end_time] %></td>
            <td><%= program[:service] %></td>
            <td><%= link_to(program[:title], program_path(program[:program_id])) %></td>
          </tr>
        <% end %>
      </tr>
    </tbody>
  </table>
<% end %>